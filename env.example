# Example environment variables
# Copy this file to .env.local for local development or set these in Vercel deployment

# NextAuth Configuration
NEXTAUTH_URL=https://cryptostarter.app
# Generate a secret with: openssl rand -base64 32
NEXTAUTH_SECRET=your_nextauth_secret_here

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Database connection
DATABASE_URL=your_database_connection_string_here

# CryptoProcessing API
CRYPTO_PROCESSING_API_KEY=your_api_key_here
CRYPTO_PROCESSING_STORE_ID=your_store_id_here
PLATFORM_ESCROW_WALLET=your_escrow_wallet_address_here
PLATFORM_WALLET_TYPE=SOL

# WalletConnect
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here 