{
  "name": "cryptostarterapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node prisma/cleanup.js && node prisma/vercel-build.js && node prisma/manual-generate.js",
    "build": "rm -rf .next && next build",
    "postbuild": "node prisma/wrap-handlers.js",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --no-engine",
    "vercel-build": "prisma generate --no-engine && next build",
    "postinstall": "prisma generate --no-engine",
    "test-db": "node prisma/test-db-connection.js",
    "update-env": "node prisma/update-env.js",
    "check-db": "node prisma/check-db.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/core": "0.34.2",
    "@auth/prisma-adapter": "^2.9.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/lab": "^7.0.0-beta.11",
    "@mui/material": "^7.0.0",
    "@mui/system": "^7.0.2",
    "@mui/x-date-pickers": "^8.0.0",
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/wallet-adapter-react-ui": "^0.9.38",
    "@solana/wallet-adapter-solflare": "^0.6.31",
    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "^1.98.2",
    "@tanstack/react-query": "^5.74.11",
    "@web3modal/wagmi": "^5.1.11",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.28.1",
    "wagmi": "^2.15.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
